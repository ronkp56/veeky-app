{
  "name": "veeky-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prepare": "husky",
    "dev": "expo start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,md,css}\" --write",
    "prettier:check": "prettier --check .",
    "doctor": "expo-doctor",
    "clean": "rimraf node_modules package-lock.json && npm i && expo start -c",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:build": "expo export --platform web",
    "build": "expo export --platform web"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@shopify/flash-list": "2.0.2",
    "expo": "~54.0.27",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-video": "~3.0.15",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "4.16.0",
    "react-native-web": "0.21.0",
    "react-native-worklets": "0.5.1",
    "undefined": "\\"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.36.0",
    "@expo/ngrok": "^4.1.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "private": true
}
